{
  "name": "@hiep20012003/joblance-shared",
  "version": "0.0.7",
  "description": "Helpers library for Joblance app",
  "license": "MIT",
  "author": "Nguyen Dung Hiep",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiep20012003/joblance-shared.git"
  },
  "keywords": [],
  "engines": {
    "node": ">=20"
  },
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      }
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --out-dir build",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --out-dir build",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@elastic/elasticsearch": "^9.1.1",
    "amqplib": "^0.10.9",
    "axios": "^1.11.0",
    "cloudinary": "^2.7.0",
    "express": "^5.1.0",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1",
    "multer": "^2.0.2",
    "piscina": "^5.1.3",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "4.1.8"
  }
}
