{
  "version": "0.0.1",
  "license": "MIT",
  "description": "Helpers library for Joblance app",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && tsc --outDir build/src && node scripts/build-package.js",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/cjs --source-maps",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel src --presets=./scripts/babel-preset.js --extensions .ts,.tsx --ignore src/**/*.specs.tsx --out-dir build/esm --source-maps"
  },
  "keywords": [],
  "name": "@hiep20012003/joblance-shared",
  "author": "Nguyen Dung Hiep",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiep20012003/joblance-shared.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "build"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "default": "./build/index.mjs"
      },
      "require": {
        "types": "./build/index.d.ts",
        "default": "./build/index.js"
      }
    }
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@elastic/elasticsearch": "^9.1.1",
    "amqplib": "^0.10.9",
    "axios": "^1.11.0",
    "cloudinary": "^2.7.0",
    "express": "^5.1.0",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1",
    "multer": "^2.0.2",
    "piscina": "^5.1.3",
    "socket.io": "^4.8.1",
    "typescript": "^5.7.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^2.0.0",
    "cross-env": "^7.0.3"
  }
}
